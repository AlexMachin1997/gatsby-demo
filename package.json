{
	"name": "gatsby-starter-hello-world",
	"private": true,
	"description": "A simplified bare-bones starter for Gatsby",
	"version": "0.1.0",
	"license": "0BSD",
	"scripts": {
		"build": "gatsby build",
		"develop": "env-cmd -f .env.development --fallback gatsby develop",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
		"start": "npm run develop",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
		"allDependencies": "npm run coreDependencies && npm run devDependencies",
		"coreDependencies": "npm install",
		"devDependencies": " npm install --only=dev",
		"lint": "eslint src/**/*.js  --fix"
	},
	"dependencies": {
		"@contentful/rich-text-react-renderer": "^14.1.1",
		"gatsby": "^2.24.41",
		"gatsby-image": "^2.4.16",
		"gatsby-plugin-react-helmet": "^3.3.10",
		"gatsby-plugin-sass": "^2.3.12",
		"gatsby-plugin-sharp": "^2.6.27",
		"gatsby-remark-images": "^3.3.25",
		"gatsby-remark-relative-images": "^0.3.0",
		"gatsby-source-contentful": "^2.3.37",
		"gatsby-source-filesystem": "^2.3.24",
		"gatsby-transformer-remark": "^2.8.28",
		"gatsby-transformer-sharp": "^2.5.13",
		"node-sass": "^4.14.1",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-helmet": "^6.1.0"
	},
	"devDependencies": {
		"env-cmd": "^10.1.0",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.21.2",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.20.0",
		"eslint-plugin-react-hooks": "^4.0.4",
		"prettier": "^2.0.5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
	},
	"bugs": {
		"url": "https://github.com/gatsbyjs/gatsby/issues"
	}
}
